{
  "functions": {
    "api/contact/main.go": {
      "runtime": "@vercel/go@3.0.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/contact",
      "destination": "/api/contact/main.go"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/www/(.*)",
      "destination": "https://idabaguspurwa.com/$1",
      "permanent": true
    }
  ]
}
